{
  "mcpServers": {
    "hexstrike-ai": {
      "command": "python3",
      "args": [
        "/path/hexstrike_mcp.py",
        "--server",
        "http://IPADDRESS:8888"
      ],
      "description": "HexStrike AI v6.0 - Advanced Cybersecurity Automation Platform. Turn off alwaysAllow if you dont want autonomous execution!",
      "timeout": 300,
      "alwaysAllow": []
    }
  },
  "clients": [
    {
      "name": "openai-gpt",
      "type": "http",
      "description": "OpenAI GPT adapter using OpenAI API",
      "enabled": true,
      "env": {
        "OPENAI_API_KEY": "env"
      },
      "meta": {
        "model": "gpt-4o-mini"
      },
      "endpoint": "local:/adapters/openai_mcp.py"
    }
  ]
}
